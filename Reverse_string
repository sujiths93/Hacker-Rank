class Solution {
public:
    string reverseString(string s) {
        if(s=="")
            return s;
        if(s==" ")
            return s;
        
        string r;
        
        for(int i=s.length()-1;i>=0;i--){
            r+=s[i];
        }
        
        return r;
        
        
    }
};
